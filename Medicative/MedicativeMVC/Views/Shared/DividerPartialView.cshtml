@model List<Setting>

<section class="service-area over-layer-default" style="background-image:url(img/bg/5.jpg);">
    <div class="container padding-bottom-none padding-top-40">
        <div class="section-content">
            <div class="row">
                <div class="col-sm-12 col-md-4">
                    <div class="service-item style-1 text-white border-right">
                        <div class="service-icon">
                            <i class="fa-sharp fa-solid fa-phone"></i>
                        </div>
                        <div class="content">
                            <h5><a asp-controller="home" asp-action="contact">Give us a Call</a></h5>
                            <p>@Model.FirstOrDefault(x => x.Key == "Phone").Value</p>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12 col-md-4">
                    <div class="service-item style-1 text-white border-right">
                        <div class="">
                            <i class="fa-solid fa-envelope"></i>
                        </div>
                        <div class="content">
                            <h5><a asp-controller="home" asp-action="contact">Send us a Message</a></h5>
                            <p>@Model.FirstOrDefault(x => x.Key == "Email").Value</p>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12 col-md-4">
                    <div class="service-item style-1 text-white">
                        <div class="">
                            <i class="fa-solid fa-location-dot"></i>
                        </div>
                        <div class="content">
                            <h5><a asp-controller="home" asp-action="contact">Visit our Location</a></h5>
                            <p>@Model.FirstOrDefault(x => x.Key == "Address").Value </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>